    <PackageReference Include="VirtoCommerce.CartModule.Core" Version="3.825.0" />
    <PackageReference Include="VirtoCommerce.FileExperienceApi.Core" Version="3.904.0" />
    <PackageReference Include="VirtoCommerce.InventoryModule.Core" Version="3.805.0" />
    <PackageReference Include="VirtoCommerce.MarketingModule.Core" Version="3.815.0" />
    <PackageReference Include="VirtoCommerce.PaymentModule.Core" Version="3.804.0" />
    <PackageReference Include="VirtoCommerce.PricingModule.Core" Version="3.809.0" />
    <PackageReference Include="VirtoCommerce.ShippingModule.Core" Version="3.806.0" />
    <PackageReference Include="VirtoCommerce.Xapi.Core" Version="3.915.0-alpha.115-vcst-3299" />
    <PackageReference Include="VirtoCommerce.XCatalog.Core" Version="3.918.0-alpha.107-vccst-3299" />
  </ItemGroup>
</Project>
